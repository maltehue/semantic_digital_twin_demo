<mujoco model="apartment">

  <include file="apartment_no_visual.xml"/>

  <include file="milk_box.xml"/>

  <include file="bowl.xml"/>

  <include file="g1.xml"/>

  <option timestep="0.005" integrator="implicitfast">
    <flag multiccd="enable" />
  </option>

